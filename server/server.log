}
  

2. Search for a character:

query {
  searchCharacters(name: "Luke") {
    name
    homeworld {
      name
      residents {
        name
      }
    }
  }
}
  

3. Add a new character:

mutation {
  addCharacter(input: {
    name: "Rey"
    height: 170
    homeworldId: 1
    starshipIds: [1]
    filmIds: [4]
  }) {
    id
    name
    homeworld {
      name
    }
  }
}
  
